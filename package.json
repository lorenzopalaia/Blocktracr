{
  "name": "blocktracr",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "predev": "npm run update",
    "dev": "next dev --turbopack",
    "postdev": "npm run format",
    "prebuild": "npm run update-data && npx prettier --write . --log-level warn && npx knip",
    "build": "next build",
    "postbuild": "npm run sitemap && npm run update-license",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write . --log-level warn",
    "knip": "knip",
    "sitemap": "next-sitemap",
    "update": "npx npm-check-updates -u && npm install",
    "update-data": "node scripts/updateData.mjs",
    "update-license": "node scripts/updateLicense.mjs",
    "tree": "npx tree-cli -a -l 3 --ignore 'node_modules, .git, .next'"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.2",
    "@tanstack/react-table": "^8.21.2",
    "ccxt": "^4.4.69",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.483.0",
    "motion": "^12.5.0",
    "next": "15.2.3",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "npm-check-updates": "^17.1.16",
    "postcss-load-config": "^6.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1",
    "shadcn": "^2.3.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tree-cli": "^0.6.7",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.3",
    "knip": "^5.46.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "typescript": "^5.8.2"
  }
}
