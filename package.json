{
  "name": "blocktracr",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "predev": "npm run update",
    "dev": "next dev --turbopack",
    "postdev": "npm run format",
    "prebuild": "npm run update-data && npx knip",
    "build": "next build --turbopack",
    "build:updated": "npm run update && npm run build",
    "build:test": "npm run build && rm -rf .next",
    "postbuild": "npm run sitemap && npm run update-license",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write . --log-level warn",
    "knip": "knip",
    "sitemap": "next-sitemap",
    "update": "npx npm-check-updates -u && npm install",
    "update-data": "node scripts/updateData.mjs",
    "update-license": "node scripts/updateLicense.mjs",
    "tree": "npx tree-cli -a -l 3 --ignore 'node_modules, .git, .next'",
    "reinstall": "rm -rf .next node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-table": "^8.21.3",
    "ccxt": "^4.4.71",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.546.0",
    "motion": "^12.6.3",
    "next": "^15.5.6",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "npm-check-updates": "^17.1.16",
    "postcss-load-config": "^6.0.1",
    "protobufjs": "^7.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^2.15.1",
    "shadcn": "^2.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tree-cli": "^0.6.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.13.17",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.2.4",
    "knip": "^5.46.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.8.2"
  }
}
