{
  "name": "blocktracr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "npm run update",
    "dev": "next dev --turbopack",
    "prebuild": "npm run update && npm run format && npm run knip",
    "build": "next build",
    "postbuild": "npm run sitemap && npm run update-data && npm run update-license",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write . --log-level warn",
    "knip": "knip",
    "sitemap": "next-sitemap",
    "update": "npx npm-check-updates -u && npm install",
    "update-data": "node scripts/updateData.mjs",
    "update-license": "node scripts/updateLicense.mjs",
    "dir-tree": "npx tree-cli -a -l 3 --ignore 'node_modules, .git, .next'"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
